<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <item>
        <shape android:shape="rectangle">
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp"></padding>
            <solid android:color="@color/list"></solid>
            <corners android:radius="300dp"></corners>
            <stroke android:width="2dp" android:color="@color/list"></stroke>
        </shape>
    </item>

</selector>